     <h4 class="page-head-line">Events in Santiago</h4>

<% @events.each do |event| %>
    <div class="container">
    <div class="row">
      <div class="[ col-xs-12 col-sm-offset-2 col-sm-8 ]">
        <ul class="event-list">
